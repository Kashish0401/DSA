class Solution
{
    static int smallestSumSubarray(int a[], int n)
    {
        int min= Integer.MAX_VALUE;
        int sum=0;
        for(int i=0;i<n;i++){
            sum+=a[i];
            if(sum<min)
                min=sum;
            if(sum>0){
                sum=0;
            }
        }
        return min;
    }
}
